{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "flashblocks/taxonomy-links",
	"version": "0.1.0",
	"title": "Taxonomy Links",
	"category": "widgets",
	"keywords": [
		"category",
		"term",
		"tag",
		"custom",
		"CPT"
	],
	"icon": "",
	"description": "Display custom taxonomy terms in posts and templates.",
	"example": {},
	"attributes": {
		"textAlign": {
			"type": "string"
		},
		"assigned": {
			"type": "boolean",
			"default": false
		},
		"showEmpty": {
			"type": "boolean",
			"default": false
		},
		"taxonomy": {
			"type": "string",
			"default": "category"
		},
		"orderby": {
			"type": "string",
			"default": ""
		},
		"terms": {
			"type": "array",
			"default": []
		},
		"container": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true,
				"link": true
			}
		},
		"align": true,
		"anchor": true,
		"ariaLabel": true,
		"html": true,
		"spacing": {
			"margin": true,
			"padding": true,
			"__experimentalDefaultControls": {
				"margin": false,
				"padding": false
			},
			"blockGap": true
		},
		"dimensions": {
			"minHeight": true
		},
		"position": {
			"sticky": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		}
	},
	"styles": [
		{
			"name": "horizontal",
			"label": "Horizontal"
		},
		{
			"name": "style1",
			"label": "Image & Button"
		},
		{
			"name": "hierarchy",
			"label": "Hierarchy"
		}
	],
	"textdomain": "flashblocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"//viewScript": "file:./view.js"
}
