{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "flashblocks/content",
	"version": "0.1.0",
	"title": "Dynamic Content",
	"category": "widgets",
	"icon": "star-filled",
	"description": "Insert dynamic content.",
	"keywords": [
		"content",
		"container"
	],
	"attributes": {
		"displayMetaData": {
			"type": "boolean",
			"default": false
		},
		"displayHidden": {
			"type": "boolean",
			"default": false
		},
		"atts": {
			"type": "string"
		}
	},
	"supports": {
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true,
				"link": true
			}
		},
		"//align": true,
		"anchor": true,
		"ariaLabel": true,
		"html": true,
		"spacing": {
			"margin": true,
			"padding": true,
			"__experimentalDefaultControls": {
				"margin": false,
				"padding": false
			},
			"blockGap": true
		},
		"dimensions": {
			"minHeight": true
		},
		"position": {
			"sticky": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		}
	},
	"//styles": [
		{
			"name": "list-meta",
			"label": "List Metadata"
		}
	],
	"textdomain": "flashblocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"//viewScript": "file:./view.js"
}
