{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "sdm/taxonomy-list",
	"version": "0.1.0",
	"title": "Taxonomy List",
	"category": "widgets",
	"icon": "category",
	"description": "List taxonomies",
	"attributes": {
		"taxonomies": {
			"type": "array",
			"default": []
		},
		"orderby": {
			"type": "string",
			"default": ""
		},
		"displayAsDropdown": {
			"type": "boolean",
			"default": false
		},
		"showHierarchy": {
			"type": "boolean",
			"default": false
		},
		"showPostCounts": {
			"type": "boolean",
			"default": false
		},
		"showOnlyTopLevel": {
			"type": "boolean",
			"default": false
		},
		"showEmpty": {
			"type": "boolean",
			"default": false
		},
		"containerElement": {
			"type": "boolean",
			"default": true
		}
	},
	"supports": {
		"align": true,
		"html": false,
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		}
	},
	"textdomain": "taxonomy-list",
	"editorScript": "file:./index.js"
}
